{
    "apiVersion": 2,
    "name": "arkhe-blocks/section-heading",
    "title": "Section Heading",
    "category": "arkhe-blocks",
    "keywords": [
        "title"
    ],
    "supports": {
        "anchor": true,
        "className": false
    },
    "attributes": {
        "textAlign": {
            "type": "string",
            "default": "center"
        },
        "content": {
            "type": "string",
            "source": "html",
            "selector": ".ark-block-heading__main",
            "default": ""
        },
        "level": {
            "type": "number",
            "default": 2
        },
        "subContent": {
            "type": "string",
            "source": "html",
            "selector": ".ark-block-heading__sub",
            "default": ""
        },
        "subPosition": {
            "type": "string",
            "default": "bottom"
        },
        "color": {
            "type": "string"
        },
        "lineColor": {
            "type": "string"
        },
        "useLine": {
            "type": "boolean",
            "default": true
        }
    },
    "example": {
        "attributes": {
            "content": "Sectin Heading",
            "subContent": "sub text"
        }
    },
    "editorScript": "file:../../../../dist/gutenberg/blocks/section-heading/index.js",
    "style": "arkhe-blocks-section-heading-style",
    "textdomain": "arkhe-blocks"
}
